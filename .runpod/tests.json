{
  "tests": [
    {
      "name": "ace_step_basic_test",
      "input": {
        "workflow": {
          "1": {
            "inputs": {
              "dcae_checkpoint": "ace_step/all_in_one/ace_step_v1_3.5b.safetensors",
              "vocoder_checkpoint": "ace_step/all_in_one/ace_step_v1_3.5b.safetensors",
              "ace_step_checkpoint": "ace_step/all_in_one/ace_step_v1_3.5b.safetensors",
              "text_encoder_checkpoint": "ace_step/all_in_one/ace_step_v1_3.5b.safetensors",
              "cpu_offload": false,
              "torch_compile": false
            },
            "class_type": "ACEModelLoader",
            "_meta": {
              "title": "ACE Model Loader"
            }
          },
          "2": {
            "inputs": {
              "audio_duration": 10,
              "infer_step": 8,
              "guidance_scale": 7.0,
              "scheduler_type": "euler",
              "cfg_type": "apg",
              "omega_scale": 10.0,
              "seed": 42,
              "guidance_interval": 0.5,
              "guidance_interval_decay": 0.0,
              "min_guidance_scale": 3.0,
              "use_erg_tag": true,
              "use_erg_lyric": false,
              "use_erg_diffusion": true,
              "oss_steps": 0,
              "guidance_scale_text": 0.0,
              "guidance_scale_lyric": 0.0
            },
            "class_type": "GenerationParameters",
            "_meta": {
              "title": "Generation Parameters"
            }
          },
          "3": {
            "inputs": {
              "models": ["1", 0],
              "prompt": "upbeat electronic dance music",
              "negative_prompt": "noise, distortion",
              "lyrics": "",
              "parameters": ["2", 0],
              "overlapped_decode": false
            },
            "class_type": "ACEStepGen",
            "_meta": {
              "title": "ACE Step Generate"
            }
          },
          "4": {
            "inputs": {
              "audio": ["3", 0]
            },
            "class_type": "PreviewAudio",
            "_meta": {
              "title": "Preview Audio"
            }
          }
        }
      },
      "timeout": 300000
    }
  ],
  "config": {
    "gpuTypeId": "NVIDIA GeForce RTX 4090",
    "gpuCount": 1,
    "env": [
      {
        "key": "REFRESH_WORKER",
        "value": "false"
      }
    ],
    "allowedCudaVersions": ["12.7", "12.6"]
  }
}
